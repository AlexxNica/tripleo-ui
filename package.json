{
  "name": "tripleo_ui",
  "version": "0.0.1",
  "description": "TripleO UI - TripleO-based OpenStack deployment UI",
  "main": "index.js",
  "dependencies": {
    "babel": "~5.5.8",
    "react": "~0.13.3",
    "classnames": "~2.1.2",
    "react-mixin": "~1.6.0",
    "react-router": "~0.13.3",
    "flux": "~2.0.3",
    "when": "~3.7.3",
    "reqwest": "~2.0.1",
    "xhr2": "~0.1.2",
    "lodash": "~3.10.1"
  },
  "devDependencies": {
    "gulp": "~3.9.0",
    "gulp-rename": "~1.2.2",
    "gulp-uglify": "~1.2.0",
    "gulp-webpack": "~1.5.0",
    "browser-sync": "~2.7.12",
    "babel-core": "~5.5.8",
    "node-libs-browser": "~0.5.2",
    "webpack": "~1.9.11",
    "babel-loader": "~5.1.4",
    "gulp-less": "~3.0.3",
    "eslint": "~1.1.0",
    "babel-eslint": "~4.0.5",
    "eslint-plugin-react": "~3.2.2",
    "jest-cli": "~0.4.18",
    "babel-jest": "~5.3.0",
    "gulp-shell": "~0.4.2"
  },
  "scripts": {
    "test": "jest",
    "testdbg": "node debug --harmony ./node_modules/jest-cli/bin/jest.js --runInBand",
    "lint": "eslint src/js"
  },
  "jest": {
    "scriptPreprocessor": "../node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "rootDir": "src",
    "unmockedModulePathPatterns": [
      "babel/polyfill",
      "capitalize",
      "classnames",
      "flux",
      "react",
      "react/addons",
      "react-mixin",
      "react-router",
      "reqwest",
      "when"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtomasek/tripleo_ui"
  },
  "keywords": [
    "tuskar-ui-extras",
    "tuskar-ui"
  ],
  "author": "Jiri Tomasek jtomasek@redhat.com",
  "license": "MIT"
}
